{% for item in collections.sections %}
  {% if item.data.type === 'ourfirstcall' %}
    <div id="ourfirstcall" class="flex flex-col items-center justify-center w-full min-h-screen bg-teal-900">
      <div class="self-center w-full px-4 xl:w-1/3 sm:px:0">
        <h2 class="mb-4 text-4xl font-bold tracking-widest text-center text-gray-200">{{ item.data.title }}</h2>
        <div class="text-xl font-light text-gray-400">
          {{ item.templateContent | safe }}
        </div>
      <div class="flex flex-row justify-center w-full pt-6 text-justify">
        <a class="px-10 py-2 text-lg text-gray-200 rounded-full shadow-md bg-amber-600 hover:bg-gray-800 hover:border-red" href="https://calendly.com/terezaif/tereza-coaching">Book our first call</a>
      </div>
      </div>
    </div>
  {% endif %}
{% endfor %}