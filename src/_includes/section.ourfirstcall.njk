{% for item in collections.sections %}
  {% if item.data.type === 'ourfirstcall' %}
  <div id="ourfirstcall" class="container mx-auto  self-center items-center  justify-center py-24 px-6 ">
    <div class="text-center px-16 py-12">
     <h2 class="mb-4 text-4xl font-bold tracking-widest text-center text-eden-800 pb-12">{{ item.data.title }}</h2>
      <div class="text-xl font-medium  text-eden-900">
          {{ item.templateContent | safe }}
        </div>
    </div>
    <div class="grid justify-center">
      <img src="/img/shine-support-operator-helps-to-solve-the-problem-1.webp" alt="AWE.SOME header">
    </div>
  </div>
  <div class="grid justify-center pb-24">
  <a class="m-20 px-10 py-2 text-lg text-cornflowerlilac-300 rounded-full shadow-md bg-gray-900 hover:bg-white hover:text-gray-900"
    href="https://calendly.com/terezaif/tereza-coaching">Book a call</a>
    </div>
</div>
  {% endif %}
{% endfor %}